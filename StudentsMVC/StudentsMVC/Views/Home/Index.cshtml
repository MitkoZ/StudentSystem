@using StudentsMVC.Models
@model Student[]
@{
    ViewBag.Title = "Index";
}

<style>
    table, td, th {
    border: 1px solid black;
}
</style>
@section header {
    <h1>
        Student System
    </h1>
}

@section footer {
    @DateTime.Now.Year
}
<table>
    <tr>
        <td>Name</td>
        <td>FacultyNumber</td>
    </tr>
    @for (int i = 0; i < Model.Length; i++)
    {
    <tr>
        <td> @Model[i].Name</td>
        <td>@Model[i].FacultyNumber</td>
    </tr>
    }

</table>